



<h1>Add Student</h1>

<form method="post" asp-action="AddStudent" asp-controller="Admin" enctype="multipart/form-data">
    <div class="form-group row">
        <label  class="col-sm-2 col-form-label">First Name</label>
        <div class="col-sm-10">
            <input type="text" readonly class="form-control-plaintext"  value="Enter First Name">
        </div>
    </div>
    <div class="form-group row">
        <label  class="col-sm-2 col-form-label">Last Name</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" placeholder="Enter Last Name">
        </div>
    </div>


    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Date Of Birth</label>
        <div class="col-sm-10">
            <input type="date" class="form-control" placeholder="Enter Date Of Birth">
        </div>
    </div>


    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Photo</label>
        <div class="col-sm-10">
            <input type="file" class="form-control" for="form-data" placeholder="Enter Photo">
        </div>
    </div>

    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Email</label>
        <div class="col-sm-10">
            <input type="email" class="form-control"  placeholder="Enter Email">
        </div>
    </div>




</form>